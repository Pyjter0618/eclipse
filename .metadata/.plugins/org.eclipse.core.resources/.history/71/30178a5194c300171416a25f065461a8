
public class Hello {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       
		int sum=0;
        
		for(int i=1; i<=10; i++) {
			System.out.print(sum);
			System.out.print(" ");
			System.out.println(i);
			sum=sum+i;
		}
		System.out.print("Ostateczny winik:");
		System.out.print(sum);
	}

}
